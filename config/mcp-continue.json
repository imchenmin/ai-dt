{
  "mcpServers": {
    "ai-dt-prompt-generator": {
      "command": "python",
      "args": [
        "-m",
        "src.mcp_server"
      ],
      "env": {
        "PYTHONPATH": ".",
        "LIBCLANG_PATH": "/usr/lib/llvm-10/lib/libclang.so.1"
      },
      "cwd": "/path/to/ai-dt",
      "description": "AI-DT Prompt Generator for Google Test + MockCpp"
    }
  }
}